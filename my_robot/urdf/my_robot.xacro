<?xml version='1.0'?>

<robot name="my_robot" xmlns:xacro="http://www.ros.org/wiki/xacro">

 <xacro:include filename="$(find my_robot)/urdf/my_robot.gazebo" />  

 <link name="robot_footprint"></link>

  <joint name="robot_footprint_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="robot_footprint"/>
    <child link="chassis" />
  </joint>

  <link name='chassis'>
    <pose>0 0 0.2 0 0 0</pose>
    <inertial>
      <mass value="10.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <collision name='collision'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/> 
      <geometry>
        <box size=".25 .2 .2"/>
      </geometry>
    </collision>

    <visual name='chassis_visual'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size=".25 .2 .2"/>
      </geometry>
    </visual>
  </link>

  <link name='front_left_wheel'>
     <visual>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
        <cylinder length="0.04" radius="0.05"/>
      </geometry>
    </visual>    

    <inertial>
      <mass value="5.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <collision name='front_left_collision'>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/> 
      <geometry>
         <cylinder length="0.04" radius="0.05"/>      
      </geometry>
    </collision>

  </link>

  <link name='front_right_wheel'>
     <visual>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
        <cylinder length="0.04" radius="0.05"/>
      </geometry>
    </visual>    

    <inertial>
      <mass value="5.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <collision name='front_right_collision'>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/> 
      <geometry>
         <cylinder length="0.04" radius="0.05"/>      
      </geometry>
    </collision>

  </link>

<link name='rear_left_wheel'>
     <visual>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
        <cylinder length="0.04" radius="0.05"/>
      </geometry>
    </visual>    

    <inertial>
      <mass value="5.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <collision name='rear_left_collision'>
      <origin xyz="0 0 0" rpy=" 0 1.5707 1.5707"/> 
      <geometry>
         <cylinder length="0.04" radius="0.05"/>      
      </geometry>
    </collision>

  </link>

  <link name='rear_right_wheel'>
     <visual>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/>
      <geometry>
        <cylinder length="0.04" radius="0.05"/>
      </geometry>
    </visual>    

    <inertial>
      <mass value="5.0"/>
      <origin xyz="0.0 0 0" rpy=" 0 1.5707 1.5707"/>
      <inertia
          ixx="0.1" ixy="0" ixz="0"
          iyy="0.1" iyz="0"
          izz="0.1"
      />
    </inertial>

    <collision name='rear_right_collision'>
      <origin xyz="0 0 0" rpy="0 1.5707 1.5707"/> 
      <geometry>
         <cylinder length="0.04" radius="0.05"/>      
      </geometry>
    </collision>

  </link>

  <link name='camera'>
     <visual>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size=".05 .05 .05"/>
      </geometry>
    </visual>    

    <inertial name= 'box_inertia'>
      <mass value="0.1"/>
      <geometry>
        <box size=".05 .05 .05"/>
      </geometry>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
	ixx="1e-6" ixy="0" ixz="0" 
	iyy="1e-6" iyz="0" 
	izz="1e-6"
      />
    </inertial>

    <collision name='collision'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/> 
      <geometry>
        <box size=".05 .05 .05"/>    
      </geometry>
    </collision>

  </link>

  <link name='hokuyo'>
     <visual>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <geometry>
	<mesh filename= "package://my_robot/meshes/hokuyo.dae"/>
      </geometry>
    </visual>    

    <inertial name= 'box_inertia'>
      <mass value="1e-5"/>
      <geometry>
        <box size=".05 .05 .05"/>
      </geometry>
      <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
      <inertia
	ixx="1e-6" ixy="0" ixz="0" 
	iyy="1e-6" iyz="0" 
	izz="1e-6"
      />
    </inertial>

    <collision name='lidar_collision'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/> 
      <geometry>
         <cylinder length="0.05" radius="0.1"/>      
      </geometry>
    </collision>

  </link>

  <joint type="continuous" name="front_left_wheel_hinge">
    <origin xyz="0.1 0.1 -0.1" rpy="0 0 0"/>
    <child link="front_left_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>

  <joint type="continuous" name="front_right_wheel_hinge">
    <origin xyz="0.1 -0.1 -0.1" rpy="0 0 0"/>
    <child link="front_right_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>

  <joint type="continuous" name="rear_left_wheel_hinge">
    <origin xyz="-0.1 0.1 -0.1" rpy="0 0 0"/>
    <child link="rear_left_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>

  <joint type="continuous" name="rear_right_wheel_hinge">
    <origin xyz="-0.1 -0.1 -0.1" rpy="0 0 0"/>
    <child link="rear_right_wheel"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>

  <joint type="fixed" name="camera_joint">
    <origin xyz="0.1 0 0" rpy="0 0 0"/>
    <child link="camera"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>

  <joint type="fixed" name="hokuyo_joint">
    <origin xyz="0.15 0 0.1" rpy="0 0 0"/>
    <child link="hokuyo"/>
    <parent link="chassis"/>
    <axis xyz="0 1 0" rpy="0 0 0"/>
    <limit effort="10000" velocity="1000"/>
    <dynamics damping="1.0" friction="1.0"/>
  </joint>
</robot>


